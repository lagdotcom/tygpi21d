CFLAGS = -G -I\TC\INCLUDE -I..\LIB -ml -d -w -DMEMORY_DEBUG -DFAR_MEMORY
CFLAGS_EXE = -G -L\TC\LIB -ml -d -w
DEMO_OBJS = MKDEMO.OBJ CAMPAIGN.OBJ IO.OBJ ITEMS.OBJ MONSTERS.OBJ SAVE.OBJ ZONE.OBJ
MAIN_OBJS = DOSJUN.OBJ BUFFS.OBJ CAMPAIGN.OBJ CODE.OBJ COMBAT.OBJ DJN.OBJ EVENTS.OBJ FONT.OBJ FP.OBJ GFX.OBJ GRF.OBJ INPUT.OBJ IO.OBJ ITEMS.OBJ JFIGHTER.OBJ JCLERIC.OBJ JMAGE.OBJ JBARD.OBJ JROGUE.OBJ JRANGER.OBJ JOBS.OBJ LIST.OBJ LOGGING.OBJ MAIN.OBJ MEM.OBJ MONSTERS.OBJ MUS_SNG.OBJ MUSIC.OBJ NPC.OBJ OPTIONS.OBJ PARTY.OBJ SAVE.OBJ SOUND.OBJ STATUS.OBJ STRINGS.OBJ TIMER.OBJ ZONE.OBJ
HEADERS = BUFFS.H CAMPAIGN.H CODE.H COMBAT.H COMMON.H DJN.H DOSJUN.H EVENTS.H FEATURES.H FILES.H FONT.H FP.H GFX.H GRF.H INPUT.H IO.H ITEMS.H JBARD.H JCLERIC.H JFIGHTER.H JMAGE.H JOBS.H JRANGER.H JROGUE.H LIST.H LOGGING.H MAIN.H MONSTERS.H MUSIC.H MUS_SNG.H NPC.H OPTIONS.H PARTY.H SAVE.H SOUND.H STATUS.H STRINGS.H TIMER.H TYPES.H ZONE.H

all: dosjun

dosjun: DOSJUN.EXE

DOSJUN.EXE: $(MAIN_OBJS) $(HEADERS)
	TCC $(CFLAGS_EXE) DOSJUN.OBJ DOSJUN.LIB ..\LIB\GAMELIBL.LIB

BUFFS.OBJ: BUFFS.C $(HEADERS)
	TCC $(CFLAGS) -c -oBUFFS.OBJ BUFFS.C
	TLIB DOSJUN.LIB +-BUFFS.OBJ, DOSJUN.TXT

CAMPAIGN.OBJ: CAMPAIGN.C $(HEADERS)
	TCC $(CFLAGS) -c -oCAMPAIGN.OBJ CAMPAIGN.C
	TLIB DOSJUN.LIB +-CAMPAIGN.OBJ, DOSJUN.TXT

